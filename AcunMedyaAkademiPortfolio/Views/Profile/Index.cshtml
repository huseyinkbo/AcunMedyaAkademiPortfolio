@model List<AcunMedyaAkademiPortfolio.Models.TblProfile>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<table class=" table table-bordered">
    <tr>
        <th>Id</th>
        <th>Isim</th>
        <th>Doğum Tarihi</th>
        <th>Adres</th>
        <th>Email</th>
        <th>Güncelle</th>
        <th>Sil</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ProfileId</td>
            <td>@item.Name</td>
            <td>@item.Birthday</td>
            <td>@item.Adress</td>
            <td>@item.Email</td>
            <td><a href="/Profile/UpdateProfile/@item.ProfileId" class="btn btn-warning">Güncelle</a></td>
            <td><a href="/Profile/DeleteProfile/@item.ProfileId" class="btn btn-danger">Sil</a></td>

        </tr>

    }
</table>
<td><a href="/Profile/CreateProfile" class="btn btn-info">Yeni Profil Ekle</a></td>


