@model AcunMedyaAkademiPortfolio.Models.TblProject
@{
    ViewBag.Title = "CreateProject";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Proje Ekleme</h2>
<form method="post">

    <label>Proje İsmi</label>
    @Html.TextBoxFor(x => x.ProjectName, new { @class = "form-control" })
    <br />
    <label>Proje Resmi</label>
    @Html.TextBoxFor(x => x.ProjectImageUrl, new { @class = "form-control" })
    <br />
    <label>Proje Kategori IDsi</label>
    @Html.TextBoxFor(x => x.ProjectCategoryId, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Proje Kaydet</button>


</form>



