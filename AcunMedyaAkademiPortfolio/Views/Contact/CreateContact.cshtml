@model AcunMedyaAkademiPortfolio.Models.TblContact
@{
    ViewBag.Title = "CreateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni Contact Ekleme</h2>
<form method="post">

    <label>Contact İsmi</label>
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <label>Contact Emaili</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Contact Subjecti</label>
    @Html.TextBoxFor(x => x.Subject, new { @class = "form-control" })
    <br />
    <label>Contact Açıklaması</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <label>Contact Oluşturulma Tarihi</label>
    @Html.TextBoxFor(x => x.CreateDate, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Contact Kaydet</button>


</form>
